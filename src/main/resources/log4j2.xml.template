<?xml version="1.0" encoding="UTF-8"?>
<Configuration status="WARN">
    <Appenders>
        <file name="File" filename="logs/LogFile.log">
            <PatternLayout pattern="%d{HH:mm:ss.SSS} %-5level - %msg%n"/>
        </file>
    </Appenders>
    <Loggers>
        <Root level="DEBUG">
            <AppenderRef ref="File"/>
        </Root>
    </Loggers>
</Configuration>